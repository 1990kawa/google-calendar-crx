{
  "manifest_version": 2,
  "name": "__MSG_google_calendar_extension_name__",
  "description": "__MSG_google_calendar_extension_desc__",
  "version": "0.0.2",
  "default_locale": "ja",
  "icons": {
    "16": "icons/logo_calendar_32.png",
    "48": "icons/logo_calendar_96.png",
    "128": "icons/logo_calendar_256.png"
  },
  "permissions": [
    "identity"
  ],
